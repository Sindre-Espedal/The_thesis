@online{kristensen2022,
  title = {Er det planlagt et nytt luftslott på Gismarvik?},
  author = {Kristensen, Steffen},
  date = {2022-06-08T05:21:14},
  url = {https://www.h-avis.no/5-62-1356620},
  urldate = {2023-02-10},
  abstract = {I sin tid skrev undertegnende et leserinnlegg om planlegging av diverse luftslott på Haugalandet. Siden den tid har verden blitt rammet av en pandemi, nemlig covid-19. Denne har medført at lokal og internasjonalt industri og reiseliv er blitt rammet hardt økonomisk.Det har igjen resultert i at den edrueligheten som ble etterlyst av undertegnede i forhold til alle disse luftslottene som poppet opp både i Sveio i form av «Vikingland», på Borgøy i Tysvær hvor «Borgøy Soul Resort» ble lansert og ikke minst i Karmøy kommune hvor det totalt urealistisk prosjekt kalt «Fjord Motorpark» ble båret til torgs.Ledelsen i disse luftige prosjektene sa tydeligvis alle de rette ordene til de folkevalgte i de respektive kommunene og enkelte bedrifter og klarte tilsynelatende å føre dem bak lysetNå har heldigvis denne egenskapen å sette tæring etter næring mer eller mindre tvunget seg fram i kommunebudsjettene og hos lokal industri i kjølvannet av bortfall av inntekter og påføring av uforutsette utgifter.Da er det heldigvis ikke lenger så enkelt å planlegge og å bruke andres midler. Dette er, slik jeg ser det, ikke udelt negativt da nesten alle disse luftslottene, som aldri hadde livets rett, er blitt skrinlagt ved at diverse kommunestyrer på Haugalandet og ikke minst reiselivet og lokal industri nå har fått kalt vann i årene og blitt mer realistiske i forhold til investeringskåtskap.Slik jeg ser dette har det spart både det offentlige og private virksomheter for store summer ved at investeringer som med stor sannsynlighet ikke ville vært regningsvarende er blitt skrinlagt.Nå har det imidlertid dukket opp et nytt prosjekt på Haugaland næringspark i Gismarvik i Tysvær kommune hvor vi blir fortalt at det skal investeres «et titalls milliarder» ifølge avisa, og som vår næringsminister fra nettopp vårt distrikt, Jan Christian Vestre, har trykket til sitt bryst og beskrev tidligere i begeistrede ordelag som framtidsrettet og høyaktuelt.Jeg sikter da til planene batteriselskapet Beyonde», ble grunnlagt i 2016 av Svein Kvernstuen har om å etablere en batterifabrikk. I informasjonsboksene som fulgte artiklene om selskapet i de første årene står det å lese at Beyonder arbeidet med utvikling og produksjon av miljøvennlige effektbatterier fra bl.a. sagmugg!Det ble imidlertid også da fortalt av denne gründeren fra Sandnes-området, som la fram prosjektet, at dette krevde store bidrag fra stat og kommune.Etter denne lanseringen er det blitt underlig stille rundt disse planene. Dette prosjektet får igjen alle alarmbjeller til å ringe hos undertegnede.Mon tro om ikke dette er siste eksempel på et luftslott i vårt distrikt?Jeg bare spør!},
  langid = {norsk},
  organization = {{Haugesunds Avis}}
}

